<h1>Sign up</h1>

<div class="row">
	<div class="col-md-6 offset-md-3">
		
		<%= form_for @author do |f| %>
			<% form_authenticity_token %>
					<%= render 'fields', f: f %>
					<%= f.submit "Create my account", class: "btn btn-primary" %>
		<% end %>

		<p>Already an account? <%= link_to "Sign in", new_session_path %></p>
	</div>
</div>
<%# debugger %>