<%= form_tag '/sessions' do %>
	<% form_authenticity_token %>
	<%= text_field_tag 'email', nil, placeholder: 'enter your email' %>
	<%= password_field_tag 'password', nil, placeholder: 'enter your password' %>
	<%= check_box_tag 'remember_me' %><span>remember me on this computer</span>
	<%= submit_tag 'valider', class: "btn btn-success" %>
<% end %>